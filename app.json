            "supportsTablet": true,
            "bundleIdentifier": "com.loja.solar",
            "buildNumber": "2",
            "config": {
                "googleMapsApiKey": "AIzaSyAYjbaMCvIXWuy2A1Q7lznIK08YtzlCkA0",
                "usesNonExemptEncryption": false
            }
        },
        "android": {
